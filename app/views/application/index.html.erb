<div class="video-wrapper">
  <video class="splashvid" width="100%" height="100%" autoplay loop>
    <source src="assets/videos/background.mp4" type="video/mp4">
    <!--<img src="">-->
  </video>
</div>
<div class="site-wrapper">
  <div class="site-wrapper-inner">
    <div class="cover-container">
      <div class="masthead clearfix">
        <div class="inner">
          <h3 class="masthead-brand"><%= image_tag "logo_icon_navbar.png" %>&nbsp;<%= image_tag "logo_navbar.png" %></h3>
          <ul class="nav splash masthead-nav">
            <li><a href="#">Features</a></li>
            <li><a href="#">Contact</a></li>
          </ul>
        </div>
      </div>
      <div class="inner cover">
        <h1 class="cover-heading">Get ready to prnt.</h1>
        <p class="lead">prnt is a super simple low-cost service to provide printing of stuff, wherever you are, whenever you want.</p>
        <p class="lead">
        <%= link_to new_job_path, :class => "btn btn-lg btn-default btn-huge" do %>
        <span class="glyphicon glyphicon glyphicon-print"></span>
        Print  
        <% end %>
          &nbsp;&nbsp;
        <%= link_to new_printer_path, :class => "btn btn-lg btn-default btn-huge" do %>
        <span class="glyphicon glyphicon glyphicon-share"></span>
        Share my printer  
        <% end %>
        </p>
      </div>
      <div class="mastfoot">
        <div class="inner">
          <p>Copyright 2014 - Widowmakers</p>
        </div>
      </div>
    </div>
  </div>
</div>
